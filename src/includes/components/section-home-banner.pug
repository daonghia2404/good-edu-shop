section.section-home-banner 
  .container
    .row
      .col-md-3.col-lg-3.no-padding
      .col-md-6.col-lg-6.no-padding
        .owl-carousel#home-banner-carousel.custom-carousel-style
          each item in [1,2,3,4,5]
            .item
              .image-banner-item.single
                img(src="./assets/images/image-sample-slider.jpg", alt="")

      .col-md-3.col-lg-3.no-padding
        .image-banner-item.half
          img(src="./assets/images/image-sample-slider.jpg", alt="")
        .image-banner-item.half
          img(src="./assets/images/image-sample-slider.jpg", alt="")

      .col-12.no-padding
        .image-banner-item
            img(src="./assets/images/image-long-banner.jpg", alt="")

